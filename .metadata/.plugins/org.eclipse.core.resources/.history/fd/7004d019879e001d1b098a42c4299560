package logical;

public class PrimeNumber {

	public static void main(String[] args)
	{
        //3,5,7
		
		int num=2;
		int count=0;
		
		for(int i=2; i<num; i++)
		{
			if(num%i==0)
			{
				count++;
				break;
			}
		}
      if(count==1)
      {
    	  System.out.println("Given no is not prime no");
      }
      else
      {
    	  System.out.println("Given is prime no");
    	  
      }

	}

}
